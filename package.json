{
  "name": "git-activity-logger",
  "version": "1.1.2",
  "description": "A tool to log time spent on Git branches based on activity with interactive configuration setup.",
  "main": "dist/index.js",
  "bin": {
    "git-activity-logger": "./dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "build:executables": "npm run build && pkg . --out-path ./dist-executables --targets node18-win-x64,node18-macos-x64",
    "start": "ts-node src/index.ts",
    "summary:month": "ts-node src/monthly-summary.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kacan98/track-current-task.git"
  },
  "keywords": [
    "git",
    "activity",
    "log",
    "time tracking",
    "task management"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kacan98/track-current-task/issues"
  },
  "homepage": "https://github.com/kacan98/track-current-task#readme",
  "dependencies": {
    "@types/inquirer": "^9.0.8",
    "@types/node": "^20.11.20",
    "chalk": "^4.1.2",
    "execa": "^8.0.1",
    "inquirer": "^12.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "esbuild": "^0.25.5",
    "pkg": "^5.8.1"
  }
}
