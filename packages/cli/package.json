{
  "name": "@track-current-task/cli",
  "version": "1.1.32",
  "description": "CLI time tracker for Git activity",
  "main": "../../dist/cli/index.js",
  "bin": {
    "git-activity-logger": "./../../dist/cli/packages/cli/src/index.js"
  },
  "scripts": {
    "build": "tsc",
    "build:exe": "npm run build && pkg --out-path ../../dist/cli-executables .",
    "start": "node ../../dist/cli/index.js",
    "check": "tsc --noEmit"
  },
  "dependencies": {
    "@types/inquirer": "^9.0.8",
    "chalk": "^4.1.2",
    "dotenv": "^17.2.1",
    "inquirer": "^12.6.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "nodemon": "^3.1.10",
    "pkg": "^5.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "pkg": {
    "scripts": ["../../dist/cli/**/*.js"],
    "assets": [],
    "targets": ["node18-win-x64", "node18-macos-x64"]
  }
}